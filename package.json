{
  "name": "create-juno",
  "version": "0.0.1",
  "description": "A CLI for discovering Juno.",
  "author": "David Dal Busco (https://daviddalbusco.com)",
  "license": "MIT",
  "type": "module",
  "bin": "dist/index.js",
  "main": "dist/index.js",
  "files": [
    "dist/index.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "format": "prettier . --write",
    "format:check": "prettier --check .",
    "build": "tsc --noEmit && node rmdir.mjs && node esbuild.mjs",
    "dev": "node rmdir.mjs && NODE_ENV=development node esbuild.mjs",
    "lint": "eslint --max-warnings 0 \"src/**/*\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junobuild/create-juno.git"
  },
  "bugs": {
    "url": "https://github.com/junobuild/create-juno"
  },
  "homepage": "https://juno.build",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@junobuild/utils": "^0.0.20",
    "ora": "^8.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.8",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.19.10",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-prettier": "^5.1.1",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "blockchain-as-a-service",
    "baas",
    "dapps",
    "dapps-development",
    "internet computer",
    "smart-contracts",
    "web3",
    "cli"
  ]
}
